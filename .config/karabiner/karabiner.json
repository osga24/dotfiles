                                "to": [{ "key_code": "end" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Change Fn + h/j/k/l to Arrows",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "h",
                                    "modifiers": {
                                        "mandatory": ["fn"],
                                        "optional": ["caps_lock"]
