{
  "title": "Custom: Fn + o/[ to Home/End",
  "rules": [
    {
      "description": "Change Fn + o to Home, Fn + [ to End",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "home"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "end"
            }
          ]
        }
      ]
    }
  ]
}
